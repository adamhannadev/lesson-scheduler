<div class="text-5xl bg-sky-500 border-2 shadow-lg border-black-50 text-white">
<h1>Events</h1>
</div>
<div id="events">
  <% @events.each do |event| %>
    <%= render event %>
    <p>
      <%= link_to "Show this event", event %>
    </p>
  <% end %>
</div>

<%= link_to "New event", new_event_path %>
